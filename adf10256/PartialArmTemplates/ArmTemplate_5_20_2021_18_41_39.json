{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf10256"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pl_Logging')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "LogTable",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [],
						"policy": {
							"timeout": "00:10:00",
							"retry": 3,
							"retryIntervalInSeconds": 120,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": {
								"value": "@pipeline().parameters.StoreProcedureName",
								"type": "Expression"
							},
							"storedProcedureParameters": {
								"DataFactory": {
									"value": {
										"value": "@pipeline().parameters.DataFactory",
										"type": "Expression"
									}
								},
								"Pipeline": {
									"value": {
										"value": "@pipeline().parameters.Pipeline",
										"type": "Expression"
									}
								},
								"RunId": {
									"value": {
										"value": "@pipeline().parameters.RunId",
										"type": "Expression"
									}
								},
								"TriggerType": {
									"value": {
										"value": "@pipeline().parameters.TriggerType",
										"type": "Expression"
									}
								},
								"TriggerId": {
									"value": {
										"value": "@pipeline().parameters.TriggerId",
										"type": "Expression"
									}
								},
								"TriggerName": {
									"value": {
										"value": "@pipeline().parameters.TriggerName",
										"type": "Expression"
									}
								},
								"TriggerTime": {
									"value": {
										"value": "@pipeline().parameters.TriggerTime",
										"type": "Expression"
									}
								},
								"GroupId": {
									"value": {
										"value": "@pipeline().parameters.GroupId",
										"type": "Expression"
									}
								},
								"Status": {
									"value": {
										"value": "@pipeline().parameters.Status",
										"type": "Expression"
									}
								},
								"Message": {
									"value": {
										"value": "@pipeline().parameters.Message",
										"type": "Expression"
									}
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "ln_demdb",
							"type": "LinkedServiceReference",
							"parameters": {
								"dbserver": {
									"value": "@pipeline().globalParameters.sqlconnection",
									"type": "Expression"
								},
								"ddbname": {
									"value": "@pipeline().globalParameters.DbName",
									"type": "Expression"
								}
							}
						}
					}
				],
				"parameters": {
					"StoreProcedureName": {
						"type": "String"
					},
					"DataFactory": {
						"type": "string"
					},
					"Pipeline": {
						"type": "string"
					},
					"RunId": {
						"type": "string"
					},
					"TriggerType": {
						"type": "string"
					},
					"TriggerId": {
						"type": "string"
					},
					"TriggerName": {
						"type": "string"
					},
					"TriggerTime": {
						"type": "string"
					},
					"GroupId": {
						"type": "string"
					},
					"Status": {
						"type": "string"
					},
					"Message": {
						"type": "string"
					}
				},
				"annotations": []
			},
			"dependsOn": []
		}
	]
}