{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf10256"
		},
		"ln_demdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ln_demdb'"
		},
		"ln_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ln_adlsgen2'"
		},
		"ln_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlgen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf10256_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ln_demdb_connectionString": {
						"value": "[parameters('ln_demdb_connectionString')]"
					},
					"ln_adlsgen2_accountKey": {
						"value": "[parameters('ln_adlsgen2_accountKey')]"
					},
					"ln_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ln_adlsgen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}